pipeline {
  agent any {
    stages {

      stage('Get') {
        steps {
          make build
        }
      }

      stage('Build') {
        steps {
          make build
        }
      }
      stage('Check') {
        steps {
          make check
        }
      }
      stage('Clean') {
        steps {
          make clean
        }
      }
    }
  }
}
